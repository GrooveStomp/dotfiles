!- Xft Settings ----------------------------------------------------------------
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: false

!- Urxvt Settings --------------------------------------------------------------
!URxvt.depth: 32
!URxvt.loginShell: true
!URxvt.internalBorder: 3
!URxvt.lineSpace: 0
! Old foreground: URxvt*foreground: #657b83
!URxvt*foreground: #c5cfd3
!URxvt*background: #002b36
URxvt*font: xft:Liberation Mono:pixelsize=35 !#:antialias=true:hinting=true
URxvt*boldFont: xft:Liberation Mono:bold:pixelsize=15
URxvt*scrollBar: false
!URxvt*letterSpace: -1
URxvt.iso14755: false
URxvt.iso14755_52: false

