[user]
	name = Aaron Oman
	email = aaron.oman@gmail.com
[color]
	ui = auto
[diff]
	renames = copies
[alias]
	plog = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[merge]
	conflictstyle = diff3
[include]
	path = .gitconfig-local
[pull]
	ff = only
[init]
	defaultBranch = main
